{
  "name": "backend",
  "version": "0.0.1",
  "description": "Testing",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch --preserveWatchOutput",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js --config src/config/orm.config.ts",
    "db:migrate": "npm run typeorm migration:run",
    "db:create-migration": "npm run typeorm migration:create -- -n",
    "db:generate-migration": "npm run build && npm run typeorm migration:generate -- -n",
    "db:schema-log": "npm run build && npm run typeorm schema:log",
    "db:revert": "npm run typeorm migration:revert",
    "deploy": "npm ci && npm run build && npm run start:prod"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.81.0",
    "@aws-sdk/client-secrets-manager": "^3.131.0",
    "@google-cloud/pubsub": "^3.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "8.0.4",
    "@nestjs/websockets": "^8.0.0",
    "@sendgrid/mail": "^7.6.2",
    "aws-sdk": "^2.1317.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express-basic-auth": "^1.2.1",
    "firebase-admin": "^11.0.0",
    "googleapis": "^105.0.0",
    "helmet": "^5.0.2",
    "jsforce": "^1.11.0",
    "jwks-rsa": "^2.1.0",
    "lodash.isempty": "^4.4.0",
    "lodash.isundefined": "^3.0.1",
    "lodash.omitby": "^4.6.0",
    "nanoid": "^3.3.4",
    "nestjs-pino": "^2.5.2",
    "nodemailer": "^6.7.7",
    "openai": "^3.2.1",
    "parse-database-url": "^0.3.0",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "pg": "^8.7.3",
    "pino-http": "^7.0.0",
    "pino-pretty": "^7.6.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "0.2",
    "typeorm-naming-strategies": "^4.1.0",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/jsforce": "^1.11.0",
    "@types/lodash.isempty": "^4.4.7",
    "@types/lodash.isundefined": "^3.0.7",
    "@types/lodash.omitby": "^4.6.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
